{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "ES6",
    "moduleResolution": "node",
    "outDir": "dist",
    "baseUrl": ".",
    "sourceMap": true,
    "downlevelIteration": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "module": "ES6",
    "paths": {
      "*": ["node_modules/*"],
    },
  },
  "include": ["src/*"],
}